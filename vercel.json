{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build"
      }
    ],
    "routes": [
      { "src": "/style.css", "dest": "/style.css" },
      { "src": "/script.js", "dest": "/main.js" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }